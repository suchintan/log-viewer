{
  "name": "@suchintan/log-viewer",
  "private": false,
  "version": "0.1.0",
  "description": "Drop-in React log viewer with filtering, hotspot detection, and SQL-like querying.",
  "keywords": ["logs", "viewer", "react", "observability"],
  "author": "Suchintan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suchintan/log-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/suchintan/log-viewer/issues"
  },
  "homepage": "https://github.com/suchintan/log-viewer#readme",
  "type": "module",
  "main": "./dist/log-viewer.umd.cjs",
  "module": "./dist/log-viewer.js",
  "types": "./dist/types/index.d.ts",
  "files": ["dist", "README.md", "LICENSE"],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/log-viewer.js",
      "require": "./dist/log-viewer.umd.cjs",
      "default": "./dist/log-viewer.js"
    }
  },
  "scripts": {
    "preinstall": "node ./scripts/ensurePatchPackage.js",
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "alasql": "^4.8.0"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
